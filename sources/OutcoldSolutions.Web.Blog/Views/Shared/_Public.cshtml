@using Microsoft.Web.Helpers
@{
	Layout = "~/Views/Shared/_Site.cshtml";
}

@RenderBody()

@section Head {
    @if (!string.Equals(HttpContext.Current.Request.UserHostName, "::1", StringComparison.InvariantCulture))
    {
        @Analytics.GetGoogleAsyncHtml("UA-7023371-5")
    }
}




